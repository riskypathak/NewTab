{
    "manifest_version": 2,

    "name": "The New App Launcher",
    "description": "Launches apps, the good way!",
    "version": "0.0.0.1",
	"browser_action": {
   "default_icon": "icon.png",
   "default_popup": "newtab.html",
   "newtab":"newtab.html"
  },
    "permissions": ["activeTab" ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    }
}
